{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "jest.autoEnable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.fixAll.format",
    "source.fixAll.eslint"
  ],
  "css.validate": false,
  "stylelint.enable": true,
  "cSpell.words": [
    "mapbox",
    "rehype",
    "tinytime",
    "unist"
  ],
}
